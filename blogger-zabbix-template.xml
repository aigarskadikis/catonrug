<?xml version="1.0" encoding="UTF-8"?>
<zabbix_export>
    <version>5.0</version>
    <date>2020-08-22T00:14:32Z</date>
    <groups>
        <group>
            <name>Templates/Modules</name>
        </group>
        <group>
            <name>tempo</name>
        </group>
    </groups>
    <templates>
        <template>
            <template>blogger blogspot monitoring</template>
            <name>blogger blogspot monitoring</name>
            <groups>
                <group>
                    <name>Templates/Modules</name>
                </group>
            </groups>
            <applications>
                <application>
                    <name>Zabbix raw metrics</name>
                </application>
            </applications>
            <items>
                <item>
                    <name>blogger.posts.json</name>
                    <type>HTTP_AGENT</type>
                    <key>blogger.posts.json</key>
                    <delay>1d</delay>
                    <history>7d</history>
                    <trends>0</trends>
                    <value_type>TEXT</value_type>
                    <applications>
                        <application>
                            <name>Zabbix raw metrics</name>
                        </application>
                    </applications>
                    <url>https://zbx.catonrug.net/blogger.posts.json</url>
                </item>
            </items>
            <discovery_rules>
                <discovery_rule>
                    <name>Discover blogger posts</name>
                    <type>DEPENDENT</type>
                    <key>blogger.posts</key>
                    <delay>0</delay>
                    <lifetime>0</lifetime>
                    <host_prototypes>
                        <host_prototype>
                            <host>b{#BLOGID}.p{#POSTID}</host>
                            <name>{#POSTID}</name>
                            <group_links>
                                <group_link>
                                    <group>
                                        <name>tempo</name>
                                    </group>
                                </group_link>
                            </group_links>
                            <group_prototypes>
                                <group_prototype>
                                    <name>{#BLOGID}</name>
                                </group_prototype>
                            </group_prototypes>
                            <templates>
                                <template>
                                    <name>blogger post monitoring</name>
                                </template>
                            </templates>
                            <macros>
                                <macro>
                                    <macro>{$BLOGID}</macro>
                                    <value>{#BLOGID}</value>
                                </macro>
                                <macro>
                                    <macro>{$POSTID}</macro>
                                    <value>{#POSTID}</value>
                                </macro>
                            </macros>
                        </host_prototype>
                    </host_prototypes>
                    <master_item>
                        <key>blogger.posts.json</key>
                    </master_item>
                </discovery_rule>
            </discovery_rules>
        </template>
        <template>
            <template>blogger post monitoring</template>
            <name>blogger post monitoring</name>
            <groups>
                <group>
                    <name>Templates/Modules</name>
                </group>
            </groups>
            <applications>
                <application>
                    <name>Blogger categories</name>
                </application>
                <application>
                    <name>Blogger count of categories</name>
                </application>
                <application>
                    <name>Blogger count of comments</name>
                </application>
                <application>
                    <name>Blogger post modify data</name>
                </application>
                <application>
                    <name>Blogger post published</name>
                </application>
                <application>
                    <name>Blogger post title</name>
                </application>
                <application>
                    <name>Blogger post updated</name>
                </application>
                <application>
                    <name>Blogger post url</name>
                </application>
                <application>
                    <name>Zabbix raw items</name>
                </application>
            </applications>
            <items>
                <item>
                    <name>categories[{$POSTID}]</name>
                    <type>DEPENDENT</type>
                    <key>categories[{$POSTID}]</key>
                    <delay>0</delay>
                    <history>0</history>
                    <trends>0</trends>
                    <value_type>TEXT</value_type>
                    <inventory_link>TAG</inventory_link>
                    <applications>
                        <application>
                            <name>Blogger categories</name>
                        </application>
                    </applications>
                    <preprocessing>
                        <step>
                            <type>JSONPATH</type>
                            <params>$.entry.[&quot;category&quot;].[*].term</params>
                        </step>
                    </preprocessing>
                    <master_item>
                        <key>post.raw.item[{$POSTID}]</key>
                    </master_item>
                </item>
                <item>
                    <name>count.of.categories[{$POSTID}]</name>
                    <type>DEPENDENT</type>
                    <key>count.of.categories[{$POSTID}]</key>
                    <delay>0</delay>
                    <history>7d</history>
                    <trends>0</trends>
                    <units>categories</units>
                    <inventory_link>SERIALNO_A</inventory_link>
                    <applications>
                        <application>
                            <name>Blogger count of categories</name>
                        </application>
                    </applications>
                    <preprocessing>
                        <step>
                            <type>JSONPATH</type>
                            <params>$.entry.[&quot;category&quot;].[*].term.length()</params>
                        </step>
                    </preprocessing>
                    <master_item>
                        <key>post.raw.item[{$POSTID}]</key>
                    </master_item>
                    <triggers>
                        <trigger>
                            <expression>{last()}&gt;2</expression>
                            <name>More that 2 categories detected on {$POSTID}</name>
                            <url>https://www.blogger.com/blog/post/edit/{$BLOGID}/{$POSTID}</url>
                            <priority>WARNING</priority>
                            <manual_close>YES</manual_close>
                        </trigger>
                    </triggers>
                </item>
                <item>
                    <name>post.comments[{$POSTID}]</name>
                    <type>DEPENDENT</type>
                    <key>post.comments[{$POSTID}]</key>
                    <delay>0</delay>
                    <history>7d</history>
                    <trends>0</trends>
                    <units>comments</units>
                    <inventory_link>SOFTWARE_APP_C</inventory_link>
                    <applications>
                        <application>
                            <name>Blogger count of comments</name>
                        </application>
                    </applications>
                    <preprocessing>
                        <step>
                            <type>JSONPATH</type>
                            <params>$.entry.link[?(@.rel == 'replies' &amp;&amp; @.type == 'text/html')].title</params>
                        </step>
                        <step>
                            <type>REGEX</type>
                            <params>([0-9]+)
\1</params>
                        </step>
                    </preprocessing>
                    <master_item>
                        <key>post.raw.item[{$POSTID}]</key>
                    </master_item>
                    <triggers>
                        <trigger>
                            <expression>{diff()}=1</expression>
                            <name>Count of comments has been changed on {$POSTID}</name>
                            <url>https://www.blogger.com/blog/post/edit/{$BLOGID}/{$POSTID}</url>
                            <priority>INFO</priority>
                            <manual_close>YES</manual_close>
                        </trigger>
                    </triggers>
                </item>
                <item>
                    <name>post.header.json[{$POSTID}]</name>
                    <type>HTTP_AGENT</type>
                    <key>post.header.json[{$POSTID}]</key>
                    <delay>1d</delay>
                    <history>0</history>
                    <trends>0</trends>
                    <value_type>TEXT</value_type>
                    <inventory_link>HARDWARE_FULL</inventory_link>
                    <applications>
                        <application>
                            <name>Zabbix raw items</name>
                        </application>
                    </applications>
                    <url>https://catonrug.blogspot.com/feeds/posts/default/{$POSTID}</url>
                    <query_fields>
                        <query_field>
                            <name>alt</name>
                            <value>json</value>
                        </query_field>
                    </query_fields>
                    <retrieve_mode>HEADERS</retrieve_mode>
                </item>
                <item>
                    <name>post.published[{$POSTID}]</name>
                    <type>DEPENDENT</type>
                    <key>post.published[{$POSTID}]</key>
                    <delay>0</delay>
                    <history>0</history>
                    <trends>0</trends>
                    <value_type>CHAR</value_type>
                    <inventory_link>SOFTWARE_APP_A</inventory_link>
                    <applications>
                        <application>
                            <name>Blogger post published</name>
                        </application>
                    </applications>
                    <preprocessing>
                        <step>
                            <type>JSONPATH</type>
                            <params>$.entry.published.['$t']</params>
                        </step>
                    </preprocessing>
                    <master_item>
                        <key>post.raw.item[{$POSTID}]</key>
                    </master_item>
                </item>
                <item>
                    <name>post.raw.item[{$POSTID}]</name>
                    <type>HTTP_AGENT</type>
                    <key>post.raw.item[{$POSTID}]</key>
                    <delay>1d</delay>
                    <history>0</history>
                    <trends>0</trends>
                    <value_type>TEXT</value_type>
                    <applications>
                        <application>
                            <name>Zabbix raw items</name>
                        </application>
                    </applications>
                    <timeout>1m</timeout>
                    <url>https://catonrug.blogspot.com/feeds/posts/default/{$POSTID}</url>
                    <query_fields>
                        <query_field>
                            <name>alt</name>
                            <value>json</value>
                        </query_field>
                    </query_fields>
                </item>
                <item>
                    <name>post.title[{$POSTID}]</name>
                    <type>DEPENDENT</type>
                    <key>post.title[{$POSTID}]</key>
                    <delay>0</delay>
                    <history>0</history>
                    <trends>0</trends>
                    <value_type>TEXT</value_type>
                    <inventory_link>NAME</inventory_link>
                    <applications>
                        <application>
                            <name>Blogger post title</name>
                        </application>
                    </applications>
                    <preprocessing>
                        <step>
                            <type>JSONPATH</type>
                            <params>$.entry.title['$t']</params>
                        </step>
                    </preprocessing>
                    <master_item>
                        <key>post.raw.item[{$POSTID}]</key>
                    </master_item>
                </item>
                <item>
                    <name>post.updated[{$POSTID}]</name>
                    <type>DEPENDENT</type>
                    <key>post.updated[{$POSTID}]</key>
                    <delay>0</delay>
                    <history>0</history>
                    <trends>0</trends>
                    <value_type>CHAR</value_type>
                    <inventory_link>SOFTWARE_APP_B</inventory_link>
                    <applications>
                        <application>
                            <name>Blogger post updated</name>
                        </application>
                    </applications>
                    <preprocessing>
                        <step>
                            <type>JSONPATH</type>
                            <params>$.entry.updated.['$t']</params>
                        </step>
                    </preprocessing>
                    <master_item>
                        <key>post.raw.item[{$POSTID}]</key>
                    </master_item>
                </item>
                <item>
                    <name>post.url[{$POSTID}]</name>
                    <type>DEPENDENT</type>
                    <key>post.url[{$POSTID}]</key>
                    <delay>0</delay>
                    <history>0</history>
                    <trends>0</trends>
                    <value_type>CHAR</value_type>
                    <inventory_link>LOCATION</inventory_link>
                    <applications>
                        <application>
                            <name>Blogger post url</name>
                        </application>
                    </applications>
                    <preprocessing>
                        <step>
                            <type>JSONPATH</type>
                            <params>$.entry.link[?(@.rel == 'alternate' &amp;&amp; @.type == 'text/html')].href</params>
                        </step>
                        <step>
                            <type>STR_REPLACE</type>
                            <params>\/
/</params>
                        </step>
                        <step>
                            <type>REGEX</type>
                            <params>(http.*html)
\1</params>
                        </step>
                    </preprocessing>
                    <master_item>
                        <key>post.raw.item[{$POSTID}]</key>
                    </master_item>
                </item>
            </items>
        </template>
    </templates>
</zabbix_export>
